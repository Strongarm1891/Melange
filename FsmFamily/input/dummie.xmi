<?xml version="1.0" encoding="ASCII"?>
<fsm:StateMachine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:fsm="http://fsm/"
    xsi:schemaLocation="http://fsm/ ../../fr.inria.diverse.examples.fsm.metamodel/model/fsm.ecore"
    name="Dummie">
  <states xsi:type="fsm:InitialState"
      name="Init"
      outgoing="//@transitions.0"/>
  <states xsi:type="fsm:FinalState"
      name="End"
      incoming="//@transitions.9"/>
  <states name="A"
      outgoing="//@transitions.1"
      incoming="//@transitions.0"/>
  <states name="B"
      outgoing="//@transitions.4"
      incoming="//@transitions.2"/>
  <states name="C"
      outgoing="//@transitions.5"
      incoming="//@transitions.3"/>
  <states name="D"
      outgoing="//@transitions.6"
      incoming="//@transitions.4"/>
  <states name="E"
      outgoing="//@transitions.7"
      incoming="//@transitions.5"/>
  <states name="F"
      outgoing="//@transitions.9"
      incoming="//@transitions.8"/>
  <states xsi:type="fsm:Fork"
      name="AToBC"
      outgoing="//@transitions.2 //@transitions.3"
      incoming="//@transitions.1"/>
  <states xsi:type="fsm:Join"
      name="DEToP"
      outgoing="//@transitions.8"
      incoming="//@transitions.6 //@transitions.7"/>
  <transitions
      name="x"
      target="//@states.2"
      source="//@states.0">
    <trigger
        expression="x"/>
  </transitions>
  <transitions
      name="y"
      target="//@states.8"
      source="//@states.2">
    <trigger
        expression="y"/>
  </transitions>
  <transitions
      name="ForkToB"
      target="//@states.3"
      source="//@states.8"/>
  <transitions
      name="ForkToC"
      target="//@states.4"
      source="//@states.8"/>
  <transitions
      name="z"
      target="//@states.5"
      source="//@states.3">
    <trigger
        expression="z"/>
  </transitions>
  <transitions
      name="o"
      target="//@states.6"
      source="//@states.4">
    <trigger
        expression="o"/>
  </transitions>
  <transitions
      name="DToJoin"
      target="//@states.9"
      source="//@states.5"/>
  <transitions
      name="EToJoin"
      target="//@states.9"
      source="//@states.6"/>
  <transitions
      name="p"
      target="//@states.7"
      source="//@states.9">
    <trigger
        expression="p"/>
  </transitions>
  <transitions
      name="q"
      target="//@states.1"
      source="//@states.7">
    <trigger
        expression="q"/>
  </transitions>
</fsm:StateMachine>
