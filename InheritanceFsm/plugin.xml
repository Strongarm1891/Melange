<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         point="fr.inria.diverse.melange.modeltype">
      <modeltype
            uri="http://fsmmt/"
            id="inheritancefsm.FsmMT">
      </modeltype>
      <modeltype
            uri="http://fsm2mt/"
            id="inheritancefsm.Fsm2MT">
         <subtyping
               modeltypeId="inheritancefsm.FsmMT">
         </subtyping>
         <subtyping
               modeltypeId="inheritancefsm.TimedFsmMT">
         </subtyping>
         <subtyping
               modeltypeId="inheritancefsm.TimedFsm2MT">
         </subtyping>
      </modeltype>
      <modeltype
            uri="http://timedfsmmt/"
            id="inheritancefsm.TimedFsmMT">
         <subtyping
               modeltypeId="inheritancefsm.FsmMT">
         </subtyping>
         <subtyping
               modeltypeId="inheritancefsm.Fsm2MT">
         </subtyping>
         <subtyping
               modeltypeId="inheritancefsm.TimedFsm2MT">
         </subtyping>
      </modeltype>
      <modeltype
            uri="http://timedfsm2mt/"
            id="inheritancefsm.TimedFsm2MT">
         <subtyping
               modeltypeId="inheritancefsm.FsmMT">
