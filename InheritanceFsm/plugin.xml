         <subtyping
               modeltypeId="inheritancefsm.Fsm2MT">
         </subtyping>
         <subtyping
               modeltypeId="inheritancefsm.TimedFsmMT">
         </subtyping>
      </modeltype>
   </extension>
   <extension
         point="fr.inria.diverse.melange.language">
      <language
            exactType="inheritancefsm.FsmMT"
            uri="http://fsm/"
            id="inheritancefsm.Fsm">
         <adapter
               class="inheritancefsm.fsm.adapters.fsmmt.FsmAdapter"
               modeltypeId="inheritancefsm.FsmMT">
         </adapter>
      </language>
      <language
            exactType="inheritancefsm.Fsm2MT"
            uri="http://fsm/"
            id="inheritancefsm.Fsm2">
         <adapter
               class="inheritancefsm.fsm2.adapters.fsmmt.Fsm2Adapter"
               modeltypeId="inheritancefsm.FsmMT">
         </adapter>
         <adapter
               class="inheritancefsm.fsm2.adapters.fsm2mt.Fsm2Adapter"
               modeltypeId="inheritancefsm.Fsm2MT">
         </adapter>
         <adapter
               class="inheritancefsm.fsm2.adapters.timedfsmmt.Fsm2Adapter"
               modeltypeId="inheritancefsm.TimedFsmMT">
         </adapter>
         <adapter
               class="inheritancefsm.fsm2.adapters.timedfsm2mt.Fsm2Adapter"
               modeltypeId="inheritancefsm.TimedFsm2MT">
         </adapter>
      </language>
      <language
            exactType="inheritancefsm.TimedFsmMT"
            uri="http://fsm/"
            id="inheritancefsm.TimedFsm">
         <adapter
               class="inheritancefsm.timedfsm.adapters.fsmmt.TimedFsmAdapter"
               modeltypeId="inheritancefsm.FsmMT">
         </adapter>
         <adapter
               class="inheritancefsm.timedfsm.adapters.fsm2mt.TimedFsmAdapter"
               modeltypeId="inheritancefsm.Fsm2MT">
         </adapter>
         <adapter
               class="inheritancefsm.timedfsm.adapters.timedfsmmt.TimedFsmAdapter"
               modeltypeId="inheritancefsm.TimedFsmMT">
         </adapter>
         <adapter
               class="inheritancefsm.timedfsm.adapters.timedfsm2mt.TimedFsmAdapter"
               modeltypeId="inheritancefsm.TimedFsm2MT">
         </adapter>
      </language>
      <language
            exactType="inheritancefsm.TimedFsm2MT"
            uri="http://fsm/"
            id="inheritancefsm.TimedFsm2">
         <adapter
               class="inheritancefsm.timedfsm2.adapters.fsmmt.TimedFsm2Adapter"
               modeltypeId="inheritancefsm.FsmMT">
         </adapter>
         <adapter
               class="inheritancefsm.timedfsm2.adapters.fsm2mt.TimedFsm2Adapter"
               modeltypeId="inheritancefsm.Fsm2MT">
         </adapter>
         <adapter
               class="inheritancefsm.timedfsm2.adapters.timedfsmmt.TimedFsm2Adapter"
               modeltypeId="inheritancefsm.TimedFsmMT">
         </adapter>
         <adapter
               class="inheritancefsm.timedfsm2.adapters.timedfsm2mt.TimedFsm2Adapter"
               modeltypeId="inheritancefsm.TimedFsm2MT">
         </adapter>
      </language>
   </extension>

</plugin>
